{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:check": "eslint --ext js,jsx,ts,tsx src helpers",
    "lint:fix": "npm run lint:check -- --fix",
    "ts:noWatch": "tsc --noEmit",
    "ts": "npm run start-checks && npm run ts:noWatch -- --watch",
    "dev": "npm run start-checks && nodemon -w .env -x \"dotenv webpack serve\"",
    "build": "npm run start-checks && dotenv webpack",
    "start": "http-server public -p 3002",
    "repo-fix": "node helpers/eslint.js --fix",
    "start-checks": "node helpers/start-checks.js",
    "preinstall": "npm run start-checks",
    "prepare": "node -e \"try { require('husky').install(); } catch(e) { if(e.code !== 'MODULE_NOT_FOUND') throw e; }\""
  },
  "dependencies": {
    "@react-three/drei": "9.70.3",
    "@react-three/fiber": "8.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.11",
    "three": "0.153.0"
  },
  "devDependencies": {
    "@arthurka/eslint": "3.14.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@total-typescript/ts-reset": "0.4.2",
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/dotenv-webpack": "7.0.4",
    "@types/node": "18.13.0",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/styled-components": "5.1.26",
    "@types/three": "0.152.1",
    "dotenv-cli": "7.2.1",
    "dotenv-webpack": "8.0.1",
    "esbuild-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "html-webpack-plugin": "5.5.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "nodemon": "2.0.22",
    "three-stl-loader": "1.0.6",
    "typescript": "5.1.3",
    "webpack": "5.85.0",
    "webpack-cli": "5.1.1",
    "webpack-dev-server": "4.15.0"
  }
}
