{
  "name": "@tic-tac-toe/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:check": "eslint --ext js,jsx,ts,tsx src",
    "lint:fix": "npm run lint:check -- --fix",
    "ts:noWatch": "tsc --noEmit",
    "ts": "npm run ts:noWatch -- --watch",
    "dev": "nodemon -e ts,tsx,js,jsx -w .env -w src -r dotenv/config --transpile-only src/index.ts",
    "start": "node -r dotenv/config dist",
    "build": "tsc"
  },
  "dependencies": {
    "@arthurka/ts-utils": "3.9.2",
    "@tic-tac-toe/common": "1.0.0",
    "dotenv": "16.1.3",
    "express": "4.18.2",
    "geoip-lite": "1.4.7",
    "socket.io": "4.6.2",
    "temp-object": "1.2.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.4.2",
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/express": "4.17.17",
    "@types/geoip-lite": "1.4.1",
    "@types/node": "18.13.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "undici": "5.22.1"
  }
}
