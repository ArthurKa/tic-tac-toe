{
  "name": "@tic-tac-toe/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:check": "eslint --ext js,jsx,ts,tsx src",
    "lint:fix": "npm run lint:check -- --fix",
    "ts:noWatch": "tsc --noEmit",
    "ts": "npm run ts:noWatch -- --watch",
    "dev": "nodemon -e ts,tsx -w .env -w src -r dotenv/config --transpile-only src/index.ts",
    "start": "node -r dotenv/config dist",
    "build": "tsc"
  },
  "dependencies": {
    "@tic-tac-toe/common": "1.0.0",
    "dotenv": "16.1.3"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.4.2",
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/node": "18.13.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  }
}
